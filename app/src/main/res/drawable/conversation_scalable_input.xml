<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <item
        android:left="@dimen/density_16"
        android:right="@dimen/density_12"
        android:bottom="@dimen/density_4"
        tools:width="50dp"
        tools:height="50dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
        </shape>
    </item>

    <item
        android:top="@dimen/density_12"
        android:bottom="@dimen/density_16"
        android:left="@dimen/density_4"
        android:gravity="left"
        android:width="@dimen/density_12">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
        </shape>
    </item>

    <item
        android:top="@dimen/density_12"
        android:bottom="@dimen/density_16"
        android:gravity="right"
        android:width="@dimen/density_12">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
        </shape>
    </item>

    <item
        android:width="@dimen/density_12"
        android:height="@dimen/density_12"
        android:bottom="@dimen/density_4"
        android:gravity="bottom|right">
        <vector xmlns:android="http://schemas.android.com/apk/res/android"
            android:width="@dimen/density_12"
            android:height="@dimen/density_12"
            android:viewportWidth="10.0"
            android:viewportHeight="10.0">
            <path
                android:pathData="M0,10 A10,10 0 0,0 10,0 L0,0 Z"
                android:fillColor="@color/white"/>
        </vector>
    </item>

    <item
        android:width="@dimen/density_12"
        android:height="@dimen/density_12"
        android:gravity="top|right">
        <vector xmlns:android="http://schemas.android.com/apk/res/android"
            android:width="@dimen/density_12"
            android:height="@dimen/density_12"
            android:viewportWidth="10.0"
            android:viewportHeight="10.0">
            <path
                android:pathData="M10,10 A10,10 0 0,0 0,0 L0,10 Z"
                android:fillColor="@color/white"/>
        </vector>
    </item>

    <item
        android:width="@dimen/density_12"
        android:height="@dimen/density_12"
        android:left="@dimen/density_4"
        android:gravity="top|left">
        <vector xmlns:android="http://schemas.android.com/apk/res/android"
            android:width="@dimen/density_12"
            android:height="@dimen/density_12"
            android:viewportWidth="10.0"
            android:viewportHeight="10.0">
            <path
                android:pathData="M10,0 A10,10 0 0,0 0,10 L10,10 Z"
                android:fillColor="@color/white"/>
        </vector>
    </item>


    <item
        android:width="@dimen/density_12"
        android:height="@dimen/density_12"
        android:left="@dimen/density_4"
        android:bottom="@dimen/density_4"
        android:gravity="bottom|left">
        <vector xmlns:android="http://schemas.android.com/apk/res/android"
            android:width="@dimen/density_12"
            android:height="@dimen/density_12"
            android:viewportWidth="10.0"
            android:viewportHeight="10.0">
            <path
                android:pathData="M10,10 A10,10 0 0,10,0,0 L10,0 Z"
                android:fillColor="@color/white"/>
        </vector>
    </item>
</layer-list>
